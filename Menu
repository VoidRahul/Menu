#include<stdio.h>

int main()
{
	
	printf("Pick a number: ");
	int menu=0;
	scanf("%d", &menu);
	
	switch(menu)
	{
		case 1: printf("Sandwich  RS149   ");
		break;
		case 2: printf("Burger  RS129  ");
		break;
	
		case 3: printf("Pasta  RS179   ");
		break;
		case 4: printf("French Fries  RS99  ");
		break;
		case 5: printf("Pizza  RS239  ");
		break;
		default: printf("option not on list  ");
	}
	printf("Remeber to eat something healthy once in a while. :-)");
	
	
}
